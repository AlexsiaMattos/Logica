import java.util.Scanner;
public class l1ex7 {
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        float dist;
        float vel;
        float temp;

        System.out.println("qual foi a dist√¢ncia percorrida:");
        dist = scanner.nextFloat();
        System.out.println("qual foi a velocidade de percurso: ");
        vel = scanner.nextFloat();
        temp =  dist / vel;
        System.out.println("o tempo de viagem foi: "+ temp);

    }
}
